@misc{tedxtalksProgrammingLanguageHeal2020,
  title      = {A Programming Language to Heal the Planet Together: {{Julia}}  | {{Alan Edelman}} | {{TEDxMIT}}},
  shorttitle = {A Programming Language to Heal the Planet Together},
  author     = {{TEDx Talks}},
  year       = {2020},
